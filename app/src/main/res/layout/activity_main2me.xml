<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2me"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Gbg"
    android:orientation="vertical">
    <include layout="@layout/top_layout" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollviev_me">
    <LinearLayout
        android:divider="@drawable/divider"
        android:showDividers="middle"
        android:orientation="vertical"
        android:layout_width="match_parent"

        android:layout_height="wrap_content">

        <TableRow
            android:layout_width="match_parent"
            android:gravity="center_vertical"
            android:layout_height="80dp" >
            <TableRow
                android:layout_width="80dp"
                android:gravity="center_vertical|center_horizontal"
                android:layout_height="80dp" >
                <ImageButton
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:id="@+id/id_tab_me"
                    android:layout_width="50dp"
                    android:gravity="center_vertical"
                    android:layout_height="50dp"
                    android:clickable="false"
                    android:background="@drawable/me1" />
            </TableRow>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >
                <TextView
                    android:text="未登录"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:textColor="@color/me2_tl_text"
                    android:id="@+id/text1Videw2" />
                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="40dp" >

                    <TextView
                        android:text="立即登录"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:gravity="center_vertical"
                        android:textColor="@color/colorAccent"
                        android:id="@+id/rootnow"
                        android:textAlignment="center"
                        android:textSize="24sp"
                        android:drawableRight="@drawable/ic_ab_back_holo_light_am"
                        android:typeface="sans" />
                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >
                    <TextView
                        android:text=""
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:gravity="center_vertical"
                        android:textColor="@color/colorAccent"
                        android:id="@+id/shoufei" />

                    <TextView
                        android:text=""
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:gravity="center_vertical"
                        android:textColor="@color/me2_tl_text"
                        android:id="@+id/textV1id1ew2" />
                </TableRow>
            </LinearLayout>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:background="@color/me2_Hbg"
            android:layout_height="20dp" >
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:gravity="center_horizontal"
            android:layout_height="match_parent" >
            <TextView
                android:text="使用介绍"
                android:id="@+id/button8JS"
                style="@style/text_button" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:gravity="center_horizontal"
            android:layout_height="match_parent" >

            <TextView
                android:text="帮助"
                android:id="@+id/button9ddl"
                style="@style/text_button" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:background="@color/me2_Hbg"
            android:layout_height="20dp" >
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
            <TextView
                android:text="数据更新时间："
                android:layout_width="0dp"
                android:layout_marginLeft="20dp"
                android:layout_height="35dp"
                android:layout_weight="3"
                android:textColor="@color/me2_tl_text"
                android:gravity="center_vertical"
                />
            <TextView
                android:text=""
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="5"
                android:textColor="@color/me2_tl_text"
                android:layout_height="40dp"
                android:id="@+id/textView_me" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
            <TextView
                android:text="使用期限："
                android:layout_width="0dp"
                android:layout_marginLeft="20dp"
                android:layout_height="35dp"
                android:layout_weight="3"
                android:textColor="@color/me2_tl_text"
                android:gravity="center_vertical"
                />
            <TextView
                android:text="试用"
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="5"
                android:textColor="@color/me2_tl_text"
                android:layout_height="40dp"
                android:id="@+id/textView_me1" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
            <TextView
                android:text="软件版本："
                android:layout_width="0dp"
                android:layout_marginLeft="20dp"
                android:layout_height="35dp"
                android:layout_weight="2"
                android:textColor="@color/me2_tl_text"
                android:gravity="center_vertical"
                />
            <TextView
                android:text=""
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_weight="4"
                android:id="@+id/thisbb"
                android:gravity="center_vertical"
                android:textColor="@color/me2_tl_text"
                android:textSize="18sp" />
            <TextView
                android:text="by--anang"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_weight="3"
                android:gravity="center_vertical"
                android:textColor="@color/TS" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:background="@color/me2_Hbg"
            android:layout_height="20dp" >
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:gravity="center_horizontal"
            android:layout_height="match_parent" >
            <TextView
                android:text="设置"
                android:id="@+id/checkBox_sz"
                style="@style/text_button" />
        </TableRow>


        <LinearLayout
            android:id="@+id/xgkf_layout"
            android:divider="@drawable/divider"
            android:showDividers="middle"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:text="修改库房信息："
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@color/me2_Hbg"
                    android:gravity="center_vertical"

                    android:textColor="@color/me2_tl_text"
                    android:id="@+id/textView12" />
            </TableRow>
            <TextView
                android:layout_width="0dp"
                android:layout_height="10dp"
                android:gravity="center_vertical"
                android:textColor="@color/me2_tl_text" />
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <Spinner
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:layout_height="40dp"
                    android:gravity="center_vertical"
                    style="@style/spinner_style"
                    android:id="@+id/me_spinner4" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:gravity="center_vertical|top"
                    android:id="@+id/editText"
                    android:hint="仓库名称"
                    android:textColorHint="@color/TS"
                    android:textColor="@color/me_bleak1"
                    android:layout_weight="2" />
            </TableRow>
            <TextView
                android:layout_width="0dp"
                android:layout_height="10dp"
                android:gravity="center_vertical"
                android:textColor="@color/me2_tl_text" />
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/editText12"
                    android:hint="新名可不写"
                    android:textColorHint="@color/TS"
                    android:textColor="@color/me_bleak1"
                    android:layout_weight="2" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:inputType="number"
                    android:ems="10"
                    android:id="@+id/editText1"
                    android:hint="行数"
                    android:textColorHint="@color/TS"
                    android:textColor="@color/me_bleak1"
                    android:layout_weight="1" />

                <EditText
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:inputType="number"
                    android:ems="10"
                    android:id="@+id/editText2"
                    android:hint="列数"
                    android:textColorHint="@color/TS"
                    android:textColor="@color/me_bleak1"
                    android:layout_weight="1" />

                <Button
                    android:text="添加"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:id="@+id/button3"
                    style="@style/my_button"
                    android:layout_weight="2" />

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:background="@color/me2_Hbg"
                android:layout_height="20dp" >
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:gravity="center_horizontal"
                android:layout_height="match_parent" >
                <TextView
                    android:text="联系人"
                    android:id="@+id/button10"
                    style="@style/text_button" />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:gravity="center_horizontal"
                android:layout_height="match_parent" >
                <TextView
                    android:text="刷新"
                    android:id="@+id/button9"
                    style="@style/text_button" />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:gravity="center_horizontal"
                android:layout_height="match_parent" >
                <TextView
                    android:text="备份"
                    android:id="@+id/button7"
                    style="@style/text_button" />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:gravity="center_horizontal"
                android:layout_height="match_parent" >
                <TextView
                    android:text="恢复"
                    android:id="@+id/button8"
                    style="@style/text_button" />
            </TableRow>

        </LinearLayout>
        <TableRow
            android:layout_width="match_parent"
            android:background="@color/me2_Hbg"
            android:layout_height="30dp" >
        </TableRow>

    </LinearLayout>
    </ScrollView>

    <include layout="@layout/top_layout" />

    <include layout="@layout/top_layout" />

</LinearLayout>
